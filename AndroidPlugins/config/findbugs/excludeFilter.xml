<FindBugsFilter>
    <!-- Exclude auto generated android R file from being checked -->
    <Match>
        <Class name="~([a-zA-Z0-9._]*\.R\$)[a-z]*" />
        <Bug pattern="NM_CLASS_NAMING_CONVENTION" />
    </Match>
    <Match>
        <Class name="~([a-zA-Z0-9._]*\.R\$)[a-z]*" />
        <Bug pattern="NM_FIELD_NAMING_CONVENTION" />
    </Match>
    <Match>
        <Class name="~([a-zA-Z0-9._]*\.R\$)[a-z]*" />
        <Bug pattern="MS_MUTABLE_ARRAY" />
    </Match>
    <Match>
        <Class name="~([a-zA-Z0-9._]*\.R\$)[a-z]*" />
        <Bug pattern="MS_PKGPROTECT" />
    </Match>
    <Match>
        <Class name="~([a-zA-Z0-9._]*\.Manifest\$)[a-z]*" />
        <Bug pattern="NM_CLASS_NAMING_CONVENTION" />
    </Match>
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>
    <Match>
        <Package name="~.*android.support.v7.appcompat.*" />
    </Match>
    <Match>
        <Package name="~.*com.google.android.gms.*" />
    </Match>
</FindBugsFilter>